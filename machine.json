{
  "name": "johnny-five-reporter",
  "description": "Reports Johnny-Five sensor values based on manifest in machine.json file",
  "server": "server.sitetosite.co",
  "location": "Studio-X NYC",
  "publishers":{
    "sensors": {
      "good mic": {
        "params": {
          "pin": "A0",
          "freq": 500,
          "scale": [0,10000]
        },
        "signal":{
          "type": "range",
          "default": 50
        },
        "location":{
          "x": 500,
          "y": 250
        }
      },
      "bad mic 1":{
        "params": {
          "pin": "A1",
          "freq": 500,
          "scale": [0,10000]
        },
        "signal":{
          "type": "range",
          "default": 50
        },
        "location":{
          "x": 500,
          "y": 250
        }
      },
      "bad mic 2": {
        "params": {
          "pin": "A2",
          "freq": 500,
          "scale": [0,10000]
        },
        "signal":{
          "type": "range",
          "default": 50
        },
        "location":{
          "x": 500,
          "y": 250
        }
      },
      "photoresistor": {
        "params": {
          "pin": "A4",
          "freq": 10000,
          "scale": [0,100]
        },
        "signal":{
          "type": "range",
          "default": 50
        },
        "location":{
          "x": 500,
          "y": 250
        }
      }
    }
  },
  "subscribers":{
    "signal_led": {
      "type": "led",
      "name": "signal led",
      "params": {
        "pin": 9
      },
      "signal":{
        "type": "boolean"
      }
    }
  }
    
}