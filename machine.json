{
  "name": "johnny-five-reporter",
  "description": "Reports Johnny-Five sensor values based on manifest in machine.json file",
  "server": "server.sitetosite.co",
  "location": "Studio-X NYC",
  "publishers":{
    "sensors":[],
    "button":{
      "type": "button",
      "name": "push button",
      "params": 8,
      "signal":{
        "type": "string",
        "default": "false"
      }
    }
  },
  "subscribers":{
    "signal_led": {
      "type": "led",
      "name": "signal led",
      "params": {
        "pin": 9
      },
      "signal":{
        "type": "boolean"
      }
    }
  }
    
}